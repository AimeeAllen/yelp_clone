%section#top-header.row
  %h1.col-md-2
    = link_to image_tag("header_print.gif"), home_path
  %ul.col-md-4.clearfix
    %li= link_to "User Profiles"
  #user_links.pull-right
    %ul
      %li
        %a
          - if current_user
            = link_to "Logout", logout_path, id: "header_logout"
          - else
            = link_to "Sign Up", new_user_path, id: "header_sign_up"
            = link_to "Login", sign_in_path, id: "header_login"
